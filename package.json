{
  "name": "parking-management-system",
  "version": "1.0.0",
  "description": "Smart Parking Management System with QR Code Scanning",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
